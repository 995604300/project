{
    "name": "topthink/think",
    "description": "the new thinkphp framework",
    "type": "project",
    "keywords": [
        "framework",
        "thinkphp",
        "ORM"
    ],
    "homepage": "http://thinkphp.cn/",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "wang",
            "email": "wyx2019@outlook.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "topthink/framework": "^5.0",
        "ircmaxell/random-lib": "^1.2",
        "symfony/var-dumper": "^2.8",
        "liushoukun/dawn-api": "^2.0.0",
        "phpmailer/phpmailer": "^6.0",
        "aliyuncs/oss-sdk-php": "2.0",
        "itdashu/aliyun-dysms-php-sdk": "dev-master",
        "zoujingli/pay-php-sdk": "^1.0",
        "zoujingli/wechat-php-sdk": "^1.3",
        "whichbrowser/parser": "^2.0",
        "phpoffice/phpexcel": "^1.8",
        "gmars/tp5-nestedsets": "dev-master",
        "gmars/tp5-rbac": "^1.2",
        "topthink/think-migration": "1.1.1",
        "endroid/qrcode": "^2.5",
        "phpoffice/phpspreadsheet": "^1.9",
        "ramsey/uuid": "^3.9"
    },
    "extra": {
        "think-path": "thinkphp"
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Aliyun\\": "lib/",
            "AliyunMNS\\": "lib/MNS"
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    }
}
